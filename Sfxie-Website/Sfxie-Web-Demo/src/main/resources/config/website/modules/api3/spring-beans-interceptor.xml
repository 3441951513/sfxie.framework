<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:context="http://www.springframework.org/schema/context"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
      http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
      http://www.springframework.org/schema/context
      http://www.springframework.org/schema/context/spring-context-4.0.xsd">
      
	<bean id="informInterceptorList" class="java.util.ArrayList">
	  	<constructor-arg>
	  		<list>
	  			<ref bean="timeInformInterceptor" /> 
	  		</list>
	  	</constructor-arg>
	</bean>
	<bean id="timeInformInterceptor" class="com.sfxie.website.common.mybatis.SqlTimeInformInterceptor">
		<property name="selectWarmLimitedTime" value="${mybatis.interceptor.selectWarmLimitedTime}" />
		<property name="updateWarmLimitedTime" value="${mybatis.interceptor.updateWarmLimitedTime}" />
	</bean>
</beans>